{
  "name": "@shoplflow/base",
  "version": "0.1.1",
  "type": "module",
  "license": "Apache-2.0",
  "scripts": {
    "build:tokens": "node scripts/generate-tokens.cjs",
    "type-check": "tsc --noEmit",
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles": "./dist/index.css",
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "files": [
    "dist",
    "styles"
  ],
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@emotion/react": "*",
    "@emotion/styled": "*",
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@types/node": "20.4.9",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "eslint": "^8.44.0",
    "react": "^18",
    "react-dom": "^18",
    "ts-node": "^10.9.1",
    "tsup": "7.2.0",
    "typescript": "^5.0.2"
  }
}
