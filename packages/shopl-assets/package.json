{
  "name": "@shoplflow/shopl-assets",
  "version": "0.10.3",
  "license": "Apache-2.0",
  "type": "commonjs",
  "scripts": {
    "build:icons": "scripts/build-icons.sh",
    "lint:icons": "eslint --fix ./src/icons/generated/*.{ts,tsx}",
    "build:illustrations": "scripts/build-illustrations.sh",
    "lint:illustrations": "eslint --fix ./src/illustrations/generated/*.{ts,tsx}",
    "type-check": "tsc --noEmit || true",
    "develop": "tsup --watch",
    "build:package": "rm -rf dist/ && tsup"
  },
  "exports": {
    ".": {
      "require": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "./src/index.ts",
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
      ".": {
        "import": {
          "types": "./dist/index.d.mts",
          "default": "./dist/index.mjs"
        },
        "require": {
          "types": "./dist/index.d.ts",
          "default": "./dist/index.js"
        }
      },
      "./package.json": "./package.json"
    }
  },
  "files": [
    "/dist"
  ],
  "peerDependencies": {
    "@emotion/react": "*",
    "@emotion/styled": "*",
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@svgr/cli": "8.1.0",
    "@types/node": "20.4.9",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "lodash": "4.17.21",
    "react": "^18",
    "react-dom": "^18",
    "ts-node": "^10.9.1",
    "tsup": "7.2.0",
    "typescript": "^4.9.0"
  }
}
