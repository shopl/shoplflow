{
  "name": "@shoplflow/shopl-assets",
  "version": "0.2.0",
  "license": "Apache-2.0",
  "author": {
    "name": "Velo.kim",
    "email": "velo.kim@shoplworks.com"
  },
  "scripts": {
    "build:icons": "scripts/build-icons.sh",
    "lint:icons": "eslint --fix ./src/icons/generated/*.{ts,tsx}",
    "build:illustrations": "scripts/build-illustrations.sh",
    "lint:illustrations": "eslint --fix ./src/illustrations/generated/*.{ts,tsx}",
    "type-check": "tsc --noEmit || true",
    "develop": "tsup --watch",
    "prebuild": "pnpm run build:assets",
    "build": "tsup",
    "build:assets": "pnpm build:icons && pnpm build:illustrations"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./styles": "./dist/styles/global.css",
    "./package.json": "./package.json"
  },
  "import": "./dist/index.mjs",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "require": {
          "types": "./dist/index.d.ts",
          "default": "./dist/index.js"
        },
        "import": {
          "types": "./dist/index.d.mts",
          "default": "./dist/index.mjs"
        }
      },
      "./package.json": "./package.json"
    },
    "import": "./dist/index.mjs",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts"
  },
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@svgr/cli": "8.1.0",
    "@types/node": "20.4.9",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "eslint": "^8.44.0",
    "lodash": "4.17.21",
    "react": "^18",
    "react-dom": "^18",
    "ts-node": "^10.9.1",
    "tsup": "7.2.0",
    "typescript": "^4.9.0"
  }
}
