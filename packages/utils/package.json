  },
  "main": "dist/index.cjs",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "@types/node": "^20.8.2",
    "@types/react": "^18.2.22",
    "esbuild": "0.18.17",
    "esbuild-plugin-babel": "^0.2.3",
    "esbuild-plugin-d.ts": "^1.1.0",
    "eslint": "^8.49.0",
    "react": "^18.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.3"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
