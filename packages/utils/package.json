{
  "name": "@shoplflow/utils",
  "version": "0.1.0",
  "type": "module",
  "license": "Apache-2.0",
  "scripts": {
    "type-check": "tsc --noEmit || true",
    "build:package": "node build.js",
    "dev": "node build.js --watch"
  },
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
